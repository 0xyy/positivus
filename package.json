{
  "name": "positivus",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "css-types:build": "typed-scss-modules src/components --aliasPrefixes.~styles src/styles",
    "css-types:watch": "pnpm css-types:build -w",
    "dev": "pnpm css-types:watch & next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "next lint",
    "lint:style": "stylelint \"**/*.scss\"",
    "prepare": "husky"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dot-path-value": "^0.0.10",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.77.4",
    "value": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.1",
    "stylelint": "^16.6.1",
    "stylelint-config-clean-order": "^6.0.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5"
  }
}
